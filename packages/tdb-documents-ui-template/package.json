{
  "name": "@terminusdb/terminusdb-documents-ui-template",
  "version": "6.2.4",
  "description": "SDK to build Application from terminusdb documents",
  "main": "src/index.js",
  "module": "es6/index",
  "jsnext:main": "es6/index",
  "scripts": {
    "build-es6": "rimraf es6 && cross-env NODE_ENV=es6 babel ./src -d es6 --extensions '.js'",
    "build": "npm run build-es6 && webpack --mode production",
    "lint": "eslint src",
    "autofix": "eslint src --fix",
    "sandbox": "webpack serve --mode development  --open --progress --port 3037 --host localhost --config sandbox/webpack.config.js"
  },
  "keywords": [
    "terminusdb-documents",
    "terminusdb-documents-ui",
    "terminusdb-ui",
    "terminusdb--sdk"
  ],
  "author": "Terminusdb Team",
  "license": "Apache-2.0",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-export-default-from": "^7.18.10",
    "@babel/plugin-transform-react-jsx": "^7.19.0",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "babel-loader": "^8.2.5",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "dotenv-webpack": "^8.0.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "less-loader": "^11.0.0",
    "mini-css-extract-plugin": "^2.6.1",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.20.1",
    "@rjsf/core": "^5.0.0-beta.14",
    "@rjsf/utils": "^5.0.0-beta.14",
