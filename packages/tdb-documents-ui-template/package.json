    "codemirror-graphql": "^2.0.2",
    "dotenv": "^16.0.1",
    "highlight.js": "^11.6.0",
    "html-react-parser": "^3.0.4",
    "img-loader": "^4.0.0",
    "mermaid": "^9.3.0",
    "prismjs": "^1.28.0",
    "react-bootstrap": "^2.7.0",
    "react-bootstrap-typeahead": "^6.0.0-alpha.9",
    "react-codemirror2": "^7.2.1",
    "react-diff-viewer": "^3.1.1",
